{
  "name": "egraphql-fetch",
  "version": "0.0.2",
  "description": "Thin symmetrically encrypted GraphQL client powered by fetch",
  "main": "index.js",
  "dependencies": {
    "101": "^1.5.0",
    "isomorphic-fetch": "^2.2.1"
  },
  "devDependencies": {
    "blue-tape": "^1.0.0",
    "browserify": "^13.3.0",
    "sinon": "^1.17.4",
    "sinon-as-promised": "^4.0.0",
    "standard": "^8.6.0",
    "tape": "^4.6.3",
    "uglifyify": "^3.0.4",
    "zuul": "^3.11.1"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/8..latest",
      "chrome/38..latest",
      "firefox/30..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/7",
      "android-browser/latest"
    ]
  },
  "scripts": {
    "test": "npm run lint && npm run unit",
    "unit": "tape --coverage test/*.js",
    "lint": "standard",
    "preversion": "npm run browser",
    "browser": "browserify -g uglifyify index.js > index.min.js",
    "zuul": "zuul --local 8080 -- test/pad.unit.js test/graphql-fetch.unit.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/martinheidegger/egraphql-fetch.git"
  },
  "keywords": [
    "graphql",
    "client",
    "fetch",
    "request",
    "query",
    "mutation"
  ],
  "author": "Martin Heidegger",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/martinheidegger/egraphql-fetch/issues"
  },
  "homepage": "https://github.com/martinheidegger/egraphql-fetch"
}
